# Device release name
ro.display.series=Samsung Galaxy M21

# Disable Rescue Party
persist.sys.disable_rescue=true

# For developer info, use ADB to get its property
sys.usb.controller=13200000.dwc3

# Dynamic partitions
ro.boot.dynamic_partitions=true
ro.boot.boot_devices=13520000.ufs

# ADB
ro.secure=0
ro.adb.secure=0
ro.debuggable=1
